<div class="row">
    {{#each results}}
        <div class="col-sm-4 justify-content-center">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">{{name}}</h4>
                    <small class="text-muted">{{this.formatted_address}}</small>
                </div> 
            </div>
        </div>
    {{/each}}
</div>
