<div class="row">
    {{#each venues.results}}
        <div class="col-sm-4 justify-content-center">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">{{this.name}}</h4>
                    <small class="text-muted">{{this.formatted_address}}</small>
                    <small class="text-muted">{{this.types[0]}}</small>
                    <small class="text-muted">{{this.types[1]}}</small>
                </div> 
            </div>
        </div>
    {{/each}}
</div>
